<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?attr/colorSurface"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="?android:attr/actionBarSize"
            android:background="@color/white"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_marginRight="@dimen/dp_10"
            android:orientation="horizontal">


            <Button
                android:id="@+id/reset"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                 android:backgroundTint="@color/Red"
                android:minWidth="0dp"
                android:paddingStart="@dimen/dp_16"
                android:paddingEnd="@dimen/dp_16"
                android:text="重置"

                />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:textColor="?android:textColorPrimary"
                android:textSize="@dimen/sp_18"
                app:layout_constraintBottom_toTopOf="@id/guide_header_height"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/search"
                style="?borderlessButtonStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                 android:backgroundTint="@color/MaterialBlue"
                android:minWidth="0dp"
                android:paddingStart="@dimen/dp_16"
                android:paddingEnd="@dimen/dp_16"
                android:text="查询"
                android:textColor="@color/white"
                app:layout_constraintBottom_toTopOf="@id/guide_header_height"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"

                />
        </LinearLayout>




        <com.flyco.roundview.RoundLinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:rv_isRippleEnable="false"
            app:rv_backgroundColor="@color/app_bg"
            app:rv_cornerRadius="@dimen/dp_8"
            android:layout_marginTop="@dimen/dp_2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_weight="3"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_marginRight="@dimen/dp_10"
            >
            <EditText
                android:id="@+id/tv_zhen"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@null"
                android:gravity="start"
                android:padding="8dp"
                android:hint="所属镇"
                android:textSize="@dimen/sp_14" />
        </com.flyco.roundview.RoundLinearLayout>
        <com.flyco.roundview.RoundLinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:rv_isRippleEnable="false"
            app:rv_backgroundColor="@color/app_bg"
            app:rv_cornerRadius="@dimen/dp_8"
            android:layout_marginTop="@dimen/dp_2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_weight="3"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_marginRight="@dimen/dp_10"
            >
            <EditText
                android:id="@+id/tv_cun"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@null"
                android:gravity="start"
                android:padding="8dp"
                android:hint="所属村"
                android:textSize="@dimen/sp_14" />
        </com.flyco.roundview.RoundLinearLayout>

        <com.flyco.roundview.RoundLinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:rv_isRippleEnable="false"
            app:rv_backgroundColor="@color/app_bg"
            app:rv_cornerRadius="@dimen/dp_8"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_weight="3"
            android:layout_marginLeft="@dimen/dp_10"
             android:layout_marginRight="@dimen/dp_10"
            android:layout_marginBottom="@dimen/dp_2"
            android:layout_marginTop="@dimen/dp_2"
            >
            <EditText
                android:id="@+id/tv_num"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@null"
                android:gravity="start"
                android:padding="8dp"
                android:hint="户编号"
                android:textSize="@dimen/sp_14" />
        </com.flyco.roundview.RoundLinearLayout>
        <com.flyco.roundview.RoundLinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:rv_isRippleEnable="false"
            app:rv_backgroundColor="@color/app_bg"
            app:rv_cornerRadius="@dimen/dp_8"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_weight="3"
            android:layout_marginLeft="@dimen/dp_10"
             android:layout_marginRight="@dimen/dp_10"
            android:layout_marginBottom="@dimen/dp_2"
            >
            <EditText
                android:id="@+id/tv_value"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@null"
                android:gravity="start"
                android:padding="8dp"
                android:hint="户主"
                android:textSize="@dimen/sp_14" />
        </com.flyco.roundview.RoundLinearLayout>
        <com.flyco.roundview.RoundLinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:rv_isRippleEnable="false"
            app:rv_backgroundColor="@color/app_bg"
            app:rv_cornerRadius="@dimen/dp_8"
            android:visibility="gone"
            android:layout_marginTop="@dimen/dp_2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_weight="3"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_marginRight="@dimen/dp_10"
            >
            <EditText
                android:id="@+id/tv_shifou"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@null"
                android:gravity="start"
                android:padding="8dp"
                android:hint="是否现场发放通知书"
                android:textSize="@dimen/sp_14" />
        </com.flyco.roundview.RoundLinearLayout>
        <com.flyco.roundview.RoundLinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:rv_isRippleEnable="false"
            app:rv_backgroundColor="@color/app_bg"
            app:rv_cornerRadius="@dimen/dp_8"
            android:layout_marginTop="@dimen/dp_2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_weight="3"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_marginRight="@dimen/dp_10"
            >
            <EditText
                android:id="@+id/tv_zjhm"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@null"
                android:gravity="start"
                android:padding="8dp"
                android:hint="证件号码"
                android:textSize="@dimen/sp_14" />
        </com.flyco.roundview.RoundLinearLayout>
        <com.flyco.roundview.RoundLinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:rv_isRippleEnable="false"
            app:rv_backgroundColor="@color/app_bg"
            app:rv_cornerRadius="@dimen/dp_8"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_weight="3"
            android:layout_marginLeft="@dimen/dp_10"
             android:layout_marginRight="@dimen/dp_10"
            android:layout_marginBottom="@dimen/dp_10"
            android:layout_marginTop="@dimen/dp_2"
            >
            <EditText
                android:id="@+id/tv_zt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@null"
                android:gravity="start"
                android:padding="8dp"
                android:hint="状态"
                android:textSize="@dimen/sp_14" />
        </com.flyco.roundview.RoundLinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="@color/line" />

        <ExpandableListView
            android:id="@+id/expandableListView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:dividerHeight="0.5dp"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


    </LinearLayout>

</layout>